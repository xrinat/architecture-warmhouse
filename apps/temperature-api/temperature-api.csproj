<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <!-- Используем .NET 8.0. Вы можете изменить на 7.0 или 6.0, если нужно -->
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <!-- Это гарантирует, что имя сборки (dll) будет "temperature-api" -->
    <AssemblyName>temperature-api</AssemblyName>
  </PropertyGroup>

</Project>